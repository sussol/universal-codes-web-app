{
  "name": "universal-drug-codes-web-app",
  "version": "0.1.0",
  "private": true,
  "description": "Template of react webapp for Sussol to use each time we start a new project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./ --ext=\".js,.jsx\" --ignore-pattern build --ignore-pattern __tests__/ --ignore-pattern __mocks__/",
    "start": "webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --optimize-minimize"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/js/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/js/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "exports-loader": "^0.6.4",
    "imports-loader": "^0.7.1",
    "material-ui": "^0.19.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "sussol-react-table": "^0.1.8",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "author": "Sustainable Solutions (NZ)",
