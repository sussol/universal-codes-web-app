{
  "name": "universal-drug-codes-web-app",
  "version": "0.1.0",
  "private": true,
  "description": "Template of react webapp for Sussol to use each time we start a new project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./ --ext=\".js,.jsx\" --ignore-pattern build --ignore-pattern __tests__/ --ignore-pattern __mocks__/",
    "webpack-server": "webpack-dev-server --hot --progress --colors",
    "start": "npm run webpack-server",
    "build-development": "webpack --optimize-minimize",
    "build": "cross-env NODE_ENV=production webpack --optimize-minimize"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/js/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/js/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "material-ui": "^0.16.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-loader": "^2.2.0",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "sussol-react-table": "^0.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.6.5",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^4.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.8.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^18.1.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "^15.6.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "webpack": "3.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.1",
    "whatwg-fetch": "^0.11.0"
  },
  "author": "Sustainable Solutions (NZ)",
